/*
 * YaST2: Local security configuration
 *
 * Description:
 *   Only writing client
 *
 * Authors:
 *   Michal Svec <msvec@suse.cz>
 *
 * $Id$
 */

{

textdomain "security";
include "security_io.ycp";

list args = Args();
if(size(args)>0) {
  y2debug("Only writing... (%1)",Args(0));
  list ret = SecurityWrite(Args(0));
  SecurityFinish();
  return ret;
}
else
  y2debug("NOT writing, probably some mistake...");

return false;

} /* EOF */
